    <sql id="selectTDigitalemployeeVo">
        select id, order_error1, order_complete1, order_error2, order_complete2, order_error3, order_complete3, order_error4, order_complete4, order_error5, order_complete5, order_rank, order_score, order_persent, employee_type, data_time, remark from t_digitalemployee
    </sql>

    <select id="selectTDigitalemployeeList" parameterType="TDigitalemployee" resultMap="TDigitalemployeeResult">
        <include refid="selectTDigitalemployeeVo"/>
        <where>  
            <if test="orderError1 != null  and orderError1 != ''"> and order_error1 = #{orderError1}</if>
            <if test="orderComplete1 != null  and orderComplete1 != ''"> and order_complete1 = #{orderComplete1}</if>
            <if test="orderError2 != null  and orderError2 != ''"> and order_error2 = #{orderError2}</if>
            <if test="orderComplete2 != null  and orderComplete2 != ''"> and order_complete2 = #{orderComplete2}</if>
            <if test="orderError3 != null  and orderError3 != ''"> and order_error3 = #{orderError3}</if>
