            <if test="orderPersent != null  and orderPersent != ''"> and order_persent = #{orderPersent}</if>
            <if test="employeeType != null  and employeeType != ''"> and employee_type = #{employeeType}</if>
            <if test="dataTime != null "> and data_time = #{dataTime}</if>
        </where>
        ORDER BY data_time DESC
    </select>
    
    <select id="selectTDigitalemployeeById" parameterType="String" resultMap="TDigitalemployeeResult">
        <include refid="selectTDigitalemployeeVo"/>
        where id = #{id}
    </select>
    
    <select id="selectTDigitalemployeeListByYearMonth" parameterType="TDigitalemployee" resultMap="TDigitalemployeeResult">
        <include refid="selectTDigitalemployeeVo"/>

        <where>
            <if test="dataTime != null ">
                and DATE_FORMAT(data_time,'%Y-%m') = #{dataTime}
             </if>

        </where>
    </select>
        
    <insert id="insertTDigitalemployee" parameterType="TDigitalemployee">
